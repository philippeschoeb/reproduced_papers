{
  "description": "Default configuration for the Quantum Self-Supervised Learning reproduction.",
  "seed": 42,
  "device": "cpu",
  "outdir": "outdir",
  "logging": {
    "level": "info"
  },
  "dataset": {
    "root": "data",
    "classes": 5,
    "batch_size": 256
  },
  "model": {
    "backend": "merlin",
    "width": 8,
    "loss_dim": 128,
    "batch_norm": false,
    "temperature": 0.07,
    "modes": 10,
    "no_bunching": false,
    "layers": 2,
    "encoding": "vector",
    "q_ansatz": "sim_circ_14_half",
    "q_sweeps": 1,
    "activation": "null",
    "shots": 100,
    "q_backend": "qasm_simulator",
    "save_dhs": false
  },
  "training": {
    "epochs": 2,
    "ckpt_step": 1,
    "le_epochs": 100
  }
}
