{
  "description": "BIM attack on amplitude-encoded classifier at epsilon=0.10",
  "seed": 42,
  "experiment": "attack",
  "device": "auto",
  
  "dataset": {
    "digits": [1, 9],
    "image_size": 16,
    "normalize": true,
    "root": "data",
    "batch_size": 256
  },
  
  "model": {
    "type": "amplitude_quantum",
    "input_dim": 256,
    "n_outputs": 2,
    "n_modes": 13,
    "n_photons": 3,
    "n_layers": 2,
    "computation_space": "unbunched"
  },
  
  "attack": {
    "method": "bim",
    "epsilon": 0.10,
    "num_iter": 10,
    "alpha": 0.01
  },
  
  "options": {
    "generate_figures": true,
    "load_model": "outdir/train_quantum/model.pt"
  },
  
  "logging": {
    "level": "info"
  }
}
