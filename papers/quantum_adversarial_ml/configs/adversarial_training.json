{
  "description": "Quantum Adversarial ML - adversarial_training",
  "seed": 42,
  "experiment": "defense",
  "device": "auto",
  "dataset": {
    "digits": [
      1,
      9
    ],
    "image_size": 16,
    "normalize": true,
    "root": "data",
    "download": true,
    "batch_size": 256
  },
  "model": {
    "type": "amplitude_quantum",
    "hidden_dims": [
      128,
      64
    ],
    "n_modes": 8,
    "n_photons": 2,
    "n_layers": 2
  },
  "defense": {
    "epochs": 100,
    "learning_rate": 0.005,
    "optimizer": "adam",
    "attack_method": "bim",
    "epsilon": 0.1,
    "num_iter": 3,
    "alpha": 0.05,
    "mix_ratio": 0.5
  },
  "options": {
    "save_model": true
  },
  "logging": {
    "level": "info"
  }
}