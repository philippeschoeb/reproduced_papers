{
  "implementation": "merlin",
  "datasets": ["BAS"],
  "seed": 7,
  "device": "cpu",
  "outdir": "results/example_runs",
  "training": {
    "lr": 0.01,
    "weight_decay": 0.0005,
    "gamma": 0.95,
    "epochs": 25
  },
  "runs": {
    "BAS": {
      "n_runs": 3,
      "data_source": "scratch",
      "conv_circuit": "MZI",
      "dense_circuit": "BS",
      "measure_subset": 2,
      "dense_added_modes": 2,
      "output_proba_type": "mode",
      "output_formatting": "Mod_grouping",
      "random_states": [7, 77, 777],
      "batch_size": 8,
      "results_subdir": "BAS_example"
    }
  }
}
